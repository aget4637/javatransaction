												strukpembayaran = Input.nextInt();
												switch(strukpembayaran){
												case 1 :
												System.out.println("\n==========================VIP GASOLINE PUMP========================");
												System.out.println("=       TERIMA KASIH SUDAH MENGISI BENSIN DI VIP GASOLINE PUMP    =");
												System.out.println("=                        HARAP DATANG KEMBALI			  =");
												System.out.println("=-----------------------------------------------------------------=");
												System.out.println("  DATE			: "+tgl.getTanggal()); 
												System.out.println("  TIME			: "+tgl.getWaktu());
												System.out.println("  ITEM 			: SHELL DIESEL  ");	
												System.out.println("  TOTAL LITER		: "+b											);
												System.out.println("  TOTAL DISKON		: 5% 		");
												System.out.println("  TOTAL BAYAR		: Rp."+d			       		 );
												System.out.println("  ANDA HEMAT 		: Rp."+e 	);
												System.out.println("\n=-------------- LAYANAN KONSUMEN VIP GASOLINE PUMP ---------------=");
												System.out.println("-              SMS 0813 1444 6263 CALL 021 230 700                -");
												System.out.println("-             EMAIL : CONTACT@VIPGASOLINEPUMP.CO.ID               -");
												System.out.println("===================================================================");
												return;
													case 2 :
													System.out.println("\n-----------------------------------------");
													System.out.println(" ITEM 		: SHELL DIESEL	  ");	
													System.out.println(" TOTAL LITER	: "+b );
													System.out.println(" TOTAL DISKON	: 5% ");
													System.out.println(" TOTAL BAYAR	: Rp."+d );
													System.out.println(" ANDA HEMAT	: Rp."+e );	
													System.out.println("-----------------------------------------");
													return;
													default :
													System.out.println("Input yang anda masukkan salah, harap periksa kembali");
												}
												}
											}
											else if(creditcard >= 4) {
												System.out.println(" Input yang anda masukkan salah, harap periksa kembali");
											}
										return;
										
							case 3 :
								System.out.println("\n-------------------------------");
								System.out.println("| Pilihan isi bensin :	      |");
								System.out.println("| 1. Via jumlah uang	      |");
								System.out.println("| 2. Via jumlah liter (L)     |");
								System.out.println("-------------------------------");
								System.out.print("\nMasukkan pilihan isi bensin (1/2) : ");
								isi = Input.nextInt();
								if(isi == 1) {
								System.out.print("\nMasukkan Jumlah uang (IDR) : Rp ");
								idr = Input.nextFloat();
								totalisi = idr / 9100;
								System.out.println("\n===========================================");
								System.out.println("= Pilihan proses selanjutnya              =");
								System.out.println("= 1. Cetak Struk Pembayaran               =");
								System.out.println("= 2. Selesai                              =");
								System.out.println("===========================================");
								System.out.print("Apakah anda ingin mencetak struk pembayaran (1/2) ? ");
								strukpembayaran = Input.nextInt();
								switch(strukpembayaran){
								case 1 :
								System.out.println("\n==========================VIP GASOLINE PUMP========================");
								System.out.println("=       TERIMA KASIH SUDAH MENGISI BENSIN DI VIP GASOLINE PUMP    =");
								System.out.println("=                        HARAP DATANG KEMBALI			  =");
								System.out.println("=-----------------------------------------------------------------=");
								System.out.println("  DATE				: "+tgl.getTanggal()); 
								System.out.println("  TIME				: "+tgl.getWaktu());
								System.out.println("  ITEM 				: SHELL DIESEL    ");	
								System.out.println("  TOTAL LITER YANG DI ISI	: "+totalisi	 );
								System.out.println("  TOTAL BAYAR			: Rp."+idr        );
								System.out.println("\n=-------------- LAYANAN KONSUMEN VIP GASOLINE PUMP ---------------=");
								System.out.println("-              SMS 0813 1444 6263 CALL 021 230 700                -");
								System.out.println("-             EMAIL : CONTACT@VIPGASOLINEPUMP.CO.ID               -");
								System.out.println("===================================================================");
								return;
								case 2 :
								System.out.println("--------------------------------------------");
								System.out.println(" ITEM			: SHELL DIESEL		");
								System.out.println(" TOTAL LITER YANG DI ISI	: "+totalisi	);
								System.out.println(" TOTAL BAYAR			: Rp."+idr	);
								System.out.println("--------------------------------------------");
								return;
								default :
									System.out.println("Input yang anda masukkan salah, harap periksa kembali");
									}
								}
								else if(isi == 2) {
								System.out.print("\nMasukkan Jumlah Bensin yang akan diisi (Liter) :  ");
								b = Input.nextInt();
								a = b * 9100;
								System.out.println("\nEstimasi Harga Rp."+a);
								System.out.print("\nMasukkan Jumlah Uang Tunai yang Dibayar	:   ");
								tunaicash = Input.nextInt();
								d = tunaicash-a;
								e = a-tunaicash;
								if(tunaicash <= + a){
								System.out.println("\n==========================VIP GASOLINE PUMP========================");
								System.out.println("\n   MAAF TRANSAKSI TIDAK DAPAT DI PROSES ");
								System.out.println("   KARENA UANG PEMBAYARAN ANDA KURANG Rp."+ e ); 
								System.out.println("   MOHON PERIKSA KEMBALI UANG PEMBAYARAN ANDA ");
								System.out.println("   PASTIKAN SAMA DENGAN ESTIMASI HARGA ");
								System.out.println("   ATAU MELEBIHI ESTIMASI HARGA, TERIMAKASIH ");
								System.out.println("\n===================================================================");
								}
								else if(tunaicash >= + a){
								System.out.println("\n===========================================");
								System.out.println("= Pilihan proses selanjutnya              =");
								System.out.println("= 1. Cetak Struk Pembayaran               =");
								System.out.println("= 2. Selesai                              =");
								System.out.println("===========================================");
								System.out.print(" Apakah anda ingin mencetak struk pembayaran (1/2) ? ");
								strukpembayaran = Input.nextInt();
								switch(strukpembayaran){
								case 1 :
								System.out.println("\n==========================VIP GASOLINE PUMP========================");
								System.out.println("=       TERIMA KASIH SUDAH MENGISI BENSIN DI VIP GASOLINE PUMP    =");
								System.out.println("=                        HARAP DATANG KEMBALI			  =");
								System.out.println("=     +++++++++++++++++++++++++++++++++++++++++++++++++++++++++   =");
								System.out.println("=-----------------------------------------------------------------=");
								System.out.println("  DATE			: "+tgl.getTanggal()); 
								System.out.println("  TIME			: "+tgl.getWaktu());
								System.out.println("  ITEM 			: 	SHELL DIESEL	  	");	
								System.out.println("  TOTAL LITER		: "+b			 );
								System.out.println("  TOTAL BAYAR		: Rp."+a 		 );
								System.out.println("  TOTAL TUNAI		: Rp."+tunaicash );
								System.out.println("  TOTAL KEMBALIAN	: Rp."+ d		 );
								System.out.println("\n=-------------- LAYANAN KONSUMEN VIP GASOLINE PUMP ---------------=");
								System.out.println("-              SMS 0813 1444 6263 CALL 021 230 700                -");
								System.out.println("-             EMAIL : CONTACT@VIPGASOLINEPUMP.CO.ID               -");
								System.out.println("===================================================================");
								return;
								case 2 : 
								System.out.println("--------------------------------------------");
								System.out.println(" ITEM			: SHELL DIESEL	");
								System.out.println("  TOTAL LITER		: "+b			 );
								System.out.println("  TOTAL BAYAR		: Rp."+a 		 );
								System.out.println("  TOTAL TUNAI		: Rp."+tunaicash );
								System.out.println("  TOTAL KEMBALIAN	: Rp."+ d		 );
								System.out.println("--------------------------------------------");
								return;
								default :
									System.out.println("Input yang anda masukkan salah, harap periksa kembali");
								}
								}
								}
								else if(isi >= 3) {
									System.out.println("Input yang anda masukkan salah, harap periksa kembali");
								}
							return; 
					}
		default:
		System.out.println("Input yang anda masukkan salah, harap periksa kembali");
	}
	System.out.print("\nApakah anda ingin mengulang proses transaksi ? (Y/N)= ");
        ulang =Input.next();
		}
		while(ulang.equals("Y"));
	}
}
